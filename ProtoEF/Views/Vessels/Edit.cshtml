@model ProtoEF.Models.Vessel

@{
    ViewData["Title"] = "Edit Vessel";
}

<h2>@ViewData["Title"]</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="VesselId" />
            <div class="form-group">
                <label asp-for="VesselName" class="control-label"></label>
                <input asp-for="VesselName" class="form-control" />
                <span asp-validation-for="VesselName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index"><i class="fa fa-backward"></i>&nbsp;Back to List</a>
</div>

